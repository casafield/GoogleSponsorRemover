{
    "manifest_version": 3,
    "name": "Google Sponsor Remover",
    "version": "1.0",
    "description": "Remove sponsors from Google search result.",
    "content_scripts": [{
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"]
    }],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "action": {
      "default_icon": "images/icon19.png"
    }
}
